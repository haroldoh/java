//Take 2 Strings of same length from the user and print first one in order and second string in reversed order one char by char.

import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("Give me 2 strings");
		String s = scanner.nextLine();
		String n = scanner.nextLine();
		for(int i=0; i<s.length(); i++){
		    System.out.println(s.charAt(i));
		    System.out.println(n.charAt(n.length()-1-i));
		}
	}
}
